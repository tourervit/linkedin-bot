{
   "background": {
      "persistent": true,
      "scripts": [ "jq.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "LinkedIn Helper"
   },
   "content_scripts": [ {
      "js": [ "jq.js", "sk.js", "common.js", "sn-invites.js", "li-invites.js", "chat.js",  "for-new.js" , "follow-up.js"],
      "matches": [ "https://www.linkedin.com/*" ]
   } ],
   "description": "LinkedIn Helper",
   "key": "",
   "manifest_version": 2,
   "name": "LinkedIn Helper",
   "permissions": [ "activeTab", "tabs", "http://*/*", "https://*/*", "https://dev.ovation.java.magora.team/", "notifications" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.8"
}
